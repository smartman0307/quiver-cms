fbq('init', '{{ env.facebook.analyticsId }}');

(function() {
  var track = function(eventName, parameters) {
    if (envVars.environment === 'development') {
      console.log('track', arguments);
    }
    fbq('track', eventName, parameters);
  }
  var parameters = {url: location.pathname}
  if (localStorage) {
    var referral = JSON.parse(localStorage.getItem('ngStorage-referral'));
    parameters = _.extend(parameters, referral); 
  }
  track('PageView', parameters);
})();</script>
